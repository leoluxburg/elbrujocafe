<div class="container text-center">
  <%= image_tag "logo.png", class: "logo"%>
  <br>
  <h2><i class="fas fa-bars" data-toggle="collapse" data-target="#demo"></i></h2>
  <div id="demo" class="collapse text-center">
    <h5>Inicio</h5>
    <h5>El Brujo</h5>
    <h5>Brujeria</h5>
    <h5>Contactanos</h5>
  </div>
</div>
<hr>
</div>
<div class="container">
  <div class="row">
    <div class=" text-center">
      <%= simple_form_for @product do |f| %>
      <%= f.input :name %>
      <%= f.input :style %>
      <%= f.input :price %>
      <%= f.input :description %>
      <%= f.button :submit,class: "btn btn-bro" %>
      <% end %>
    </div>
  </div>
</div>
